{
  "expo": {
    "name": "nextgen-in",
    "slug": "nextgen-in",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "nextgenin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#01C086",
          "dark": {
            "backgroundColor": "#01845D"
          }
        }
      ],
      [
        "expo-font",
        [
          "./assets/fonts/FiraSans-MediumItalic.ttf",
          "./assets/fonts/Gabarito-Regular.ttf",
          "./assets/fonts/Gabarito-Medium.ttf",
          "./assets/fonts/Gabarito-SemiBold.ttf",
          "./assets/fonts/Gabarito-Bold.ttf",
          "./assets/fonts/FiraSans-Medium.ttf",
          "./assets/fonts/FiraSans-Regular.ttf"
        ]
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
